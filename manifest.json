{
  "name": "Wanxian: Biome Generator For Minecraft Bedrock Edition",
  "short_name": "Wanxian",
  "description": "A Webapp which can be used to create custom biomes for Minecraft bedrock edition",
  "start_url": "/index.html",
  "id": "/",
  "display": "fullscreen",
  "background_color": "#EADDFF",
  "theme_color": "#6750A4",
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/icons/icon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml"
    },
    {
      "src": "/icons/icon2.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon3.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "/icons/maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Wanxian",
      "short_name": "Wanxian",
      "description": "Open Wanxian client biome activated.",
      "url": "/index.html?clientbiomes=on",
      "icons": [{ "src": "/icons/icon1.png", "sizes": "512x512" }]
    }
  ],
  "share_target": {
    "action": "/index.html",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  }
}
